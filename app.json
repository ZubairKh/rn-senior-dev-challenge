{
  "expo": {
    "$schema": "https://raw.githubusercontent.com/expo/expo/main/packages/%40expo/config-types/src/ExpoAppManifest.schema.json",
    "name": "rn-senior-dev-challenge",
    "slug": "rn-senior-dev-challenge",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      ["expo-secure-store"]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
